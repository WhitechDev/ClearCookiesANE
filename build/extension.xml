<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.mmp.ClearCookiesANE</id>
  <versionNumber>1</versionNumber>
  <platforms>

    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libClearCookiesANE.a</nativeLibrary>
            <initializer>ExtInitializer</initializer> 
            <finalizer>ExtFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-ARM"> 
        <applicationDeployment> 
            <nativeLibrary>libClearCookiesANE.jar</nativeLibrary> 
            <initializer>air.extensions.ClearCookiesExtension</initializer> 
            <finalizer>air.extensions.ClearCookiesExtension</finalizer> 
        </applicationDeployment> 
    </platform> 
    
    <platform name="default"> 
        <applicationDeployment/> 
    </platform> 
  </platforms>
</extension>